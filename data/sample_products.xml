        <!-- Sample Kenyan Pharmacy Products -->
        
        <!-- 1. Paracetamol (OTC) -->
        <record id="product_paracetamol_500mg" model="pharmacy.product">
            <field name="name">Paracetamol 500mg Tablets</field>
            <field name="ppb_number">PPB/POM/2021/001234</field>
            <field name="drug_category">otc</field>
            <field name="active_ingredient">Paracetamol</field>
            <field name="strength">500mg</field>
            <field name="dosage_form">tablet</field>
            <field name="manufacturer">GlaxoSmithKline Kenya</field>
            <field name="shelf_life_months">36</field>
            <field name="storage_conditions">Store below 25°C in a dry place</field>
        </record>
        
        <record id="product_template_paracetamol" model="product.template">
            <field name="name">Paracetamol 500mg Tablets</field>
            <field name="default_code">PARA-500</field>
            <field name="categ_id" ref="product_category_pain_relief"/>
            <field name="list_price">50.00</field>
            <field name="standard_price">30.00</field>
            <field name="type">product</field>
            <field name="uom_id" ref="uom.product_uom_unit"/>
            <field name="uom_po_id" ref="uom.product_uom_unit"/>
            <field name="sale_ok" eval="True"/>
            <field name="purchase_ok" eval="True"/>
            <field name="available_in_pos" eval="True"/>
            <field name="taxes_id" eval="[(6, 0, [ref('account.1_sale_tax_template_16')])]"/>
            <field name="pharmacy_product_id" ref="product_paracetamol_500mg"/>
        </record>
        
